<body>


        <!-- this switch turns on/off the decimal point stuff -->
        <div id="usedecimals" class="hidden">$$fieldsref{usedecimals}</div>

        
  <div id="logo">
		<a style="text-decoration: none;" title="Home" href="/cgi-bin/cclite.cgi"><span class="titletext">Cclite</span></a>

 
     <!-- search for users,trades, ads -->
        
       <div class="adsearch">
		<form class="searchform" action="/cgi-bin/cclite.cgi" method="post">
			<input name="action" value="find" type="hidden">
			<input class="search" onfocus="if(this.value == this.defaultValue) this.value = ''" value="search" name="search_string" id="search_string"  size="10" type="text">
			<select id="search_type" class="small" name="search_type">
					<option selected value='trade'>Transactions</option>
					<option value='user'>Users</option>
					<option value='ad'>Adverts</option>
	        </select>	
	        <input value="Go" class="small" type="submit" title="Look for trades, ads or users">								
		</form>
	  </div>
  
       <!-- end of autocomplete search box -->
       
			<!-- language selector version 1  -->
				<select id="language_value" class="small" onchange="change_language(this);" name="language">
					<option value='en'>English</option>
					<option value='ar'>Arabic</option>
					<option value='zh'>Chinese</option>
					<option value='de'>Deutsch</option>
					<option value='nl'>Dutch</option>
					<option value='fr'>Francais</option>
					<option value='es'>Espagnol</option>
					<option value='it'>Italiano</option>
					<option value='el'>Greek</option>
					<option value='ja'>Japanese</option>
					<option value='pt'>Portugese</option>
					<option value='th'>Thai</option>
				</select>
			<!--  language selector version 1 end  -->

   <span id="logoninfo">$$fieldsref{youare} $$fieldsref{atregistry}
      <span ><a title="Logout $$fieldsref{user}" href="/cgi-bin/cclite.cgi?action=logoff">
